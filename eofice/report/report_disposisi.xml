			                                    <td colspan="2" width="15%" style="border:1px solid black;text-align: center;"></td>
			                                </tr>
			                                <t t-set="l" t-value="l+1"/>
			                            </t>
			                            <t t-foreach="o.user_ids5" t-as="i">
			                                <tr style="border:1px solid black;">
