		<template id="report_disposisi">
			<t t-name="eofice.report_disposisi">
			    <t t-name="eofice.report_disposisi">
			        <t t-call="report.html_container">
			            <t t-foreach="docs" t-as="o">
			                <div class="page">
			                    <div class="row">
			                        <table style="width: 100%;">
			                            <tr>
			                                <td width="40%" style="text-align: left;">
			                                    <br/><span>Lembar Penerus</span>
			                                    <br/><span t-field="o.job_delegasi"/>
			                                </td>
			                                <td width="30%"/>
			                                <td style="text-align: right;" width="30%"><img src="/eofice/static/src/img/logo.png" width="200" height="45"/></td>
			                            </tr>
			                        </table>
			                        <table style="border:1px solid black;border-collapse:collapse;" width="100%">
			                            <tr style="border:1px solid black;">
			                                <td width="50%" style="border:1px solid black;text-align: center;">NO. AGENDA</td>
			                                <td colspan="4" width="20%" style="border:1px solid black;text-align: center;">DITERIMA</td>
			                                <td colspan="4" width="30%" style="border:1px solid black;text-align: center;">TINGKAT SURAT</td>

			                            </tr>
			                            <tr style="border:1px solid black;">
			                                <td width="50%" style="border:1px solid black;text-align: center;"><span t-field="o.name"/></td>
			                                <td colspan="4" width="20%" style="border:1px solid black;text-align: center;"><span t-field="o.date" t-field-options="{&quot;format&quot;: &quot;dd MMMM y&quot;}"/></td>
			                                <td width="7.5%" style="border:1px solid black;text-align: center;">R<div t-if="o.status_surat == 'rahasia'">&#10004;</div></td>
			                                <td width="7.5%" style="border:1px solid black;text-align: center;">P<div t-if="o.status_surat == 'penting'">&#10004;</div></td>
			                                <td width="7.5%" style="border:1px solid black;text-align: center;">S<div t-if="o.status_surat == 'segera'">&#10004;</div></td>
			                                <td width="7.5%" style="border:1px solid black;text-align: center;">B<div t-if="o.status_surat == 'biasa'">&#10004;</div></td>
			                            </tr>
			                            <tr style="border:1px solid black;">
			                                <td width="50%" style="border:1px solid black;text-align: center;"><br/><br/><br/><br/>DITERUSKAN KEPADA<br/><br/><br/><br/><br/></td>
			                                <td width="5%" style="border:1px solid black;text-align: center;"><br/>A<br/>C<br/>T<br/>I<br/>O<br/>N<br/><br/><br/></td>
			                                <td width="5%" style="border:1px solid black;text-align: center;">T<br/>A<br/>N<br/>G<br>G</br><br/>A<br/>P<br/>A<br/>N<br/></td>
			                                <td width="5%" style="border:1px solid black;text-align: center;"><br/><br/>I<br/>N<br/>F<br/>O<br/><br/><br/><br/></td>
			                                <td width="5%" style="border:1px solid black;text-align: center;"><br/><br/>F<br/>I<br/>L<br/>E<br/><br/><br/><br/></td>
			                                <td colspan="2" width="15%" style="border:1px solid black;text-align: center;"><br/><br/><br/><br/>PARAF<br/><br/><br/><br/><br/></td>
			                                <td colspan="2" width="15%" style="border:1px solid black;text-align: center;"><br/><br/><br/><br/>TANGGAL DITERUSKAN<br/><br/><br/><br/><br/></td>
			                            </tr>
			                            <t t-set="l" t-value="1" />
			                            <t t-foreach="o.user_ids1" t-as="i">
			                                <tr style="border:1px solid black;">
			                                    <td width="50%" style="border:1px solid black;">
			                                        <span t-esc="l"/>. <t t-esc="i.name"/>
			                                    </td>
			                                    <td width="5%" style="border:1px solid black;text-align: center;">&#10004;</td>
			                                    <td width="5%" style="border:1px solid black;text-align: center;"></td>
			                                    <td width="5%" style="border:1px solid black;text-align: center;"></td>
			                                    <td width="5%" style="border:1px solid black;text-align: center;"></td>
			                                    <td colspan="2" width="15%" style="border:1px solid black;text-align: center;"></td>
			                                    <td colspan="2" width="15%" style="border:1px solid black;text-align: center;"></td>
			                                </tr>
			                                <t t-set="l" t-value="l+1"/>
			                            </t>
			                            <t t-foreach="o.user_ids2" t-as="i">
			                                <tr style="border:1px solid black;">
