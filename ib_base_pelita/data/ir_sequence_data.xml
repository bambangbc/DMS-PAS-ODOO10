<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for flight.requisition -->
        <record id="seq_flight_requisition" model="ir.sequence">
            <field name="name">Flight Requisition</field>
            <field name="code">flight.requisition</field>
            <field name="prefix">REQ/MKT/</field>
            <field name="padding">3</field>
            <field name="suffix">/%(month)s/%(range_year)s</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- SO Sequences -->
        <record id="seq_sales_order_pelita" model="ir.sequence">
            <field name="name">Sales Order - Pelita</field>
            <field name="code">sale.order.pelita</field>
            <field name="prefix">SO/%(range_y)s/%(month)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <!-- Flight Schedule Sequences -->
        <record id="seq_flight_schedule" model="ir.sequence">
            <field name="name">Flight Schedule</field>
            <field name="code">flight.schedule</field>
            <field name="prefix">FS/%(range_y)s/%(month)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <!-- Quotation Sequences -->
        <record id="seq_quotation_pelita" model="ir.sequence">
            <field name="name">Quotation</field>
            <field name="code">quotation.pelita</field>
            <!--<field name="prefix">FS/%(range_y)s/%(month)s/</field>-->
            <field name="padding">3</field>
            <field name="suffix">/MG/PAF/PAS/%(range_year)s</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
